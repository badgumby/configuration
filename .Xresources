! HiDPI
Xft.dpi: 128
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! rofi
rofi.dpi: 150

! urxvt settings ------------------------------------------------

! scroll settings
URxvt*saveLines:	10000
URxvt*scrollbar:	true
URxvt*thickness:	10
URxvt*scrollColor:	#1E1E1E
URxvt*scrollBar_right:	true

URxvt*font:	xft:DejaVu Sans Mono for Powerline:size=12

! URxvt terminal colors
URxvt.transparent:	true
URxvt.shading:      	30
URxvt.depth:		32
! URxvt.background:     #000000
URxvt.foreground:       #ffffff
URxvt.color0:           #3f3f3f
URxvt.color1:           #705050
URxvt.color2:           #00b45a
URxvt.color3:           #dfcf8f
URxvt.color4:           #506070
URxvt.color5:           #dc8cc3
URxvt.color6:           #8cd0d3
URxvt.color7:           #dcdccc
URxvt.color8:           #709080
URxvt.color9:           #cc5454
URxvt.color10:          #4d9f4d
URxvt.color11:          #f0dfaf
URxvt.color12:          #4186be
URxvt.color13:          #ec93d3
URxvt.color14:          #93e0e3
URxvt.color15:          #000000

! end urxvt settings -------------------------------------------
